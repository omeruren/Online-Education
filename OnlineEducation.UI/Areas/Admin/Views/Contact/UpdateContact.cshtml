@model UpdateContactDto
@{
    ViewData["Title"] = "Update Contact";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<form method="post">
	<input hidden asp-for="ContactId"/>
	<label>Address</label>
	<input type="text" asp-for="Address" class="form-control" />
	<br />
	<label>Phone</label>
	<input type="text" asp-for="Phone" class="form-control" />
	<br />
	<label>Email</label>
	<input rows="5" type="text" asp-for="Email" class="form-control" />
	<br />
	<label>Map Url</label>
	<input type="text" asp-for="MapUrl" class="form-control" />
	<br />
	<button class="btn btn-outline-primary">Update</button>
</form>
